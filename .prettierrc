{
    "plugins": ["prettier-plugin-jinja-template"],
    "overrides": [
        {
            "files": ["settings.json", ".prettierrc"],
            "options": {
                "parser": "json5",
                "quoteProps": "preserve",
                "singleQuote": false,
                "trailingComma": "all",
            },
        },
        {
            "files": ["*.html"],
            "options": {
                "parser": "jinja-template",
            },
        },
    ],
}
